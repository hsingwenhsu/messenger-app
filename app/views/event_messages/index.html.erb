<p id="notice"><%= notice %></p>

<h1>Event Messages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_messages.each do |event_message| %>
      <tr>
        <td><%= link_to 'Show', event_message %></td>
        <td><%= link_to 'Edit', edit_event_message_path(event_message) %></td>
        <td><%= link_to 'Destroy', event_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Message', new_event_message_path %>
